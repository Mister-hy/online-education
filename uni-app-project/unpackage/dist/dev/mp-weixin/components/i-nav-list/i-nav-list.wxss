
.nav-wrapper {
	width: 100%;
	height: 308rpx;
	margin: 40rpx 0;
	display: flex;
	flex-wrap: wrap;
}
.nav-item {
	width: 25%;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
}
.nav-item image {
	width: 70rpx;
	height: 70rpx;
	border-radius: 50%;
	padding: 10rpx 0;
}
.nav-item {
	color: #6c757d;
	font-size: 24rpx;
}

